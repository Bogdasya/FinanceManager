<div class="table-responsive" id="income_table">
  <table class="table table-hover">
    <thead>
    <tr>
      <th>#</th>
      <th>Источник расхода</th>
      <th>Сумма</th>
    </tr>
    </thead>
    <% object.expenses.each do |expenses| %>
        <tbody>
        <tr>
          <td><%= expenses.id %></td>
          <td><%= expenses.title %></td>
          <td><%= expenses.sum %></td>
          <td><%= link_to "Удалить", [@user, expenses], method: :delete %></td>
        </tr>
        </tbody>
    <% end %>
  </table>
</div>