<div class="table-responsive" id="income_table">
  <table class="table table-hover">
    <thead>
    <tr>
      <th>#</th>
      <th>Название</th>
    </tr>
    </thead>
    <% object.charge_elements.each do |element| %>
        <tbody>
        <tr>
          <td><%= element.id %></td>
          <td><%= element.name %></td>
          <td><%= link_to "Удалить", [@user, object ,element], method: :delete %></td>
        </tr>
        </tbody>
    <% end %>
  </table>
</div>